<!DOCTYPE html>
<title>{{ title }}</title>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<link rel="stylesheet" href="{{ baseAssets }}css/app.css"/>

<div class="sf_outer_wrapper">
  {{> partials/nav}}

  <div class="sf_wrapper">
    <div class="sf_main_wrapper">
      <div class="sf_main">
        <div class="sf_region6">
          <h1>{{ title }}</h1>
          <div class="sf_content">
            <form action="" class="contact">
              <table class="form" cellspacing="1" cellpadding="3">
                <tr>
                  <th colspan="2">
                    <p>{{ form-heading }}</p>
                  </th>
                </tr>

                <tr>
                  <td><label for="first-name">{{ first-name }}:</label></td>
                  <td><input type="text" id="first-name"></td>
                </tr>
                <tr>
                  <td><label for="last-name">{{ last-name }}:</label></td>
                  <td><input id="last-name" type="text"></td>
                </tr>
                <tr>
                  <td><label for="address-one">{{ address-one }}:</label></td>
                  <td><input id="address-one" type="text" size="30"></td>
                </tr>
                <tr>
                  <td><label for="address-two">{{ address-two }}:</label></td>
                  <td><input id="address-two" type="text" size="30"></td>
                </tr>
                <tr>
                  <td><label for="city">{{ city }}:</label></td>
                  <td><input id="city" type="text"></td>
                </tr>
                <tr>
                  <td><label for="zip">{{ zip }}:</label></td>
                  <td><input id="zip" type="text" size="5" maxlength="5"></td>
                </tr>
                <tr>
                  <td><label for="state">{{ state }}:</label></td>
                  <td>
                    <select id="state" type="text">
                      {{#each states}}
                        <option value="{{ this }}">{{ this }}</option>
                      {{/each}}
                    </select>
                  </td>
                </tr>
                <tr>
                  <td><label for="phone-one">{{ phone-one }}:</label></td>
                  <td><input id="phone-one" type="text"></td>
                </tr>
                <tr>
                  <td><label for="phone-two">{{ phone-two }}:</label></td>
                  <td><input id="phone-two" type="text"></td>
                </tr>
                <tr>
                  <td><label for="email">{{ email }}:</label></td>
                  <td><input id="email" type="text"></td>
                </tr>
                <tr>
                  <td><label for="question">{{ question }}:</label></td>
                  <td>
                    <textarea id="question" rows="4" cols="38">{{ question-template }}</textarea>
                  </td>
                </tr>

              </table>

              <p align="center">
                <input type="submit" value="Submit">
                <input type="reset" value="Reset">
              </p>
            </form>
          </div>
        </div>
      </div>
      <!-- /sf_main_wrapper -->
    </div>
    <!-- /sf_main -->
  </div>
</div>
<!-- /sf_outer_wrapper -->
{{> partials/footer}}
